{
  "name": "git-marauder",
  "description": "Encrypt secrets",
  "version": "1.0.0",
  "author": "Thomas Pucci and Tycho Tatitscheff",
  "bin": {
    "git-marauder": "./bin/run"
  },
  "bugs": "https://github.com/bamlab/git-marauder/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/bamlab/git-marauder",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "git-marauder",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bamlab/git-marauder",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
