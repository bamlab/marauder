{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "semi": [2, "always"],
    "quotes":[2, "double"],
    "object-curly-spacing": [2, "always"],
    "no-unused-vars": [0],
    "@typescript-eslint/no-unused-vars": [2, {"argsIgnorePattern": "^_", "varsIgnorePattern": "^_"}],
    "arrow-parens": [2, "always"],
    "no-use-before-define": 0,
    "comma-dangle": [2, "only-multiline"]
  }
}
